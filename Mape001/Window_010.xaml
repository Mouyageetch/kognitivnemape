                <Image Cursor="Hand" x:Name="nextWindow" Source="Images/next.png" Width="80" Height="80" VerticalAlignment="Top" HorizontalAlignment="Right" MouseLeftButtonDown="NextWindow_MouseLeftButtonDown"/>
            </DockPanel>

        </Grid>
        <Image x:Name="ravnaPovrsValjkaLines" HorizontalAlignment="Left" Height="320" Margin="907.25,210,0,0" VerticalAlignment="Top" Width="180.5" Source="Images/Granice/ravnaPovrsValjkaLines.png" Visibility="Hidden"/>
